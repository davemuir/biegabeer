msgid "Language"
msgstr "שפה"

#: ../wp-twitter-button.php:498
msgid "English"
msgstr "אנגלית"

#: ../wp-twitter-button.php:499
msgid "French"
msgstr "צרפתית"

#: ../wp-twitter-button.php:500
msgid "German"
msgstr "גרמנית"

#: ../wp-twitter-button.php:501
msgid "Spanish"
msgstr "ספרדית"

#: ../wp-twitter-button.php:502
msgid "Japanese"
msgstr "יפנית"

#: ../wp-twitter-button.php:504
msgid "The language of the button."
msgstr "השפה של הכפתור."

